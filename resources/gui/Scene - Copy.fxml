<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane prefHeight="300.0" prefWidth="425.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.GuiController">
   <bottom>
      <VBox prefHeight="0.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <StackPane prefHeight="150.0" prefWidth="999999.0">
                     <children>
                        <Button fx:id="buttonStartProcessing" disable="true" mnemonicParsing="false" onAction="#startFileAnalysis" prefHeight="30.0" prefWidth="113.0" text="Analyser filene" StackPane.alignment="CENTER_LEFT">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <Button fx:id="buttonChooseFiles" alignment="CENTER" mnemonicParsing="false" onAction="#chooseFiles" prefHeight="30.0" prefWidth="113.0" text="Velg filer" StackPane.alignment="CENTER_RIGHT">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <ImageView fx:id="imageLoading" fitHeight="62.0" fitWidth="85.0" pickOnBounds="true" preserveRatio="true" visible="false" StackPane.alignment="CENTER">
                           <image>
                              <Image url="@loading.gif" />
                           </image>
                        </ImageView>
                     </children>
                     <padding>
                        <Insets left="60.0" right="60.0" />
                     </padding>
                  </StackPane>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="30.0" prefWidth="999999.0">
               <children>
                  <Label fx:id="labelFeedbackMsg" alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="348.0" textAlignment="CENTER" textFill="#1548ff" wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
         </children>
      </VBox>
   </bottom>
   <center>
      <VBox BorderPane.alignment="TOP_CENTER">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Label fx:id="labelHelp2" alignment="CENTER" prefHeight="27.0" prefWidth="9999999.0" text="(Bruk CTRL + venstre museklikk i explorer for Ã¥ velge flere filer)" textAlignment="CENTER">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <Label fx:id="labelHelp" alignment="CENTER" contentDisplay="TOP" text="Velg en eller flere lydfiler med tale">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" />
         </children>
      </VBox>
   </center>
   <top>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <MenuBar prefHeight="25.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                    <menus>
                      <Menu mnemonicParsing="false" text="Fil">
                        <items>
                          <MenuItem mnemonicParsing="false" text="Lukk" />
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Hjelp">
                        <items>
                          <MenuItem mnemonicParsing="false" text="Om oss" />
                        </items>
                      </Menu>
                    </menus>
                  </MenuBar>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="imageLogo" fitHeight="206.0" fitWidth="348.0" pickOnBounds="true">
                     <image>
                        <Image url="@logo.jpg" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
         </children>
      </VBox>
   </top>
</BorderPane>
